@inject NavBreadcrumbService navBreadcrumb

@attribute [Authorize]

<PageTitle>C3P1.net/Cats</PageTitle>

<Cats />

@code {
    protected override void OnInitialized()
    {
        // setup breadcrumbs
        navBreadcrumb.SetBreadcrumbs(new List<Node>()
